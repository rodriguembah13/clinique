<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        {#{{ encore_entry_link_tags('global') }}#}
        <link rel="stylesheet" href="{{ asset('/build/global.css') }}">
            <!-- Custom Theme Style -->
            <link href="{{ asset('/build/dataTables.bootstrap4.mincss.css') }}" rel="stylesheet">
            <link href="{{ asset('/build/bootstrap-datetimepickercss.css') }}" rel="stylesheet">
            <link href="{{ asset('/build/adminlte.css') }}" rel="stylesheet">
            <link href="{{ asset('/build/font-awesome.css') }}" rel="stylesheet">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>

    <body class="hold-transition sidebar-mini">
    <div class="wrapper">


        <!-- Navbar -->
        {% include 'default/navbar.html.twig' %}
        <!-- /.navbar -->

        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="{{ path('homepage') }}" class="brand-link">
                <img src="dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">AdminLTE 3</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Alexander Pierce</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
               {# {% include 'default/sidebar.html.twig' %}#}
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
{% include 'default/menu.html.twig' %}
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
<br>
            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    {% block body %}

                    {% endblock %}
                </div>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2014-2018 <a href="http://adminlte.io">AdminLTE.io</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.0.0-alpha
            </div>
        </footer>

        {% block javascripts %}
            <script src="{{ asset('/build/app.js') }}"></script>
            <script src="{{ asset('/build/jquery.dataTables.js') }}"></script>
            <script src="{{ asset('/build/bootstrap-datetimepickerjs.js') }}"></script>
            <script src="{{ asset('/build/dataTables.bootstrap4.minjs.js') }}"></script>
            <script src="{{ asset('/build/adminltejs.js') }}"></script>
           {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </div> </body>
</html>
